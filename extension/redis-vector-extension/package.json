{
  "name": "redis-vector-connector",
  "displayName": "Redis Vector DB Connector",
  "description": "Connect to Redis Vector Database with SSL authentication and vector search capabilities",
  "version": "1.0.0",
  "engines": {
    "vscode": "^1.60.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:redisVector.connect"
  ],
  "main": "./out/src/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "redisVector.connect",
        "title": "Connect to Redis Vector DB",
        "category": "Redis Vector"
      },
      {
        "command": "redisVector.fetchData",
        "title": "Fetch Data from Redis",
        "category": "Redis Vector"
      },
      {
        "command": "redisVector.storeWithEmbedding",
        "title": "Store Description with Embedding",
        "category": "Redis Vector"
      },
      {
        "command": "redisVector.vectorSearch",
        "title": "Vector Search in Redis",
        "category": "Redis Vector"
      },
      {
        "command": "redisVector.disconnect",
        "title": "Disconnect from Redis",
        "category": "Redis Vector"
      }
    ],
    "configuration": {
      "title": "Redis Vector DB",
      "properties": {
        "redisVector.host": {
          "type": "string",
          "default": "nfrtredis-dev-12030.ms.com",
          "description": "Redis host address"
        },
        "redisVector.port": {
          "type": "number",
          "default": 12030,
          "description": "Redis port number"
        },
        "redisVector.caCertPath": {
          "type": "string",
          "default": "M:\\dist\\sec\\PROJ\\pki\\prod\\truststores\\GenPop-PROD\\combined\\pem\\bundle.pem",
          "description": "Path to CA certificate (Windows format)"
        },
        "redisVector.clientCertPath": {
          "type": "string",
          "default": "/var/tmp/jaisoman-push/CERTS/mycert.cer",
          "description": "Remote path to client certificate"
        },
        "redisVector.clientKeyPath": {
          "type": "string",
          "default": "/var/tmp/jaisoman-push/CERTS/mkey.key",
          "description": "Remote path to client key"
        },
        "redisVector.dbName": {
          "type": "string",
          "default": "NYTD_LC_SYSDICT",
          "description": "Database name"
        },
        "redisVector.sshHost": {
          "type": "string",
          "default": "ivapp123456.devin3.ms.com",
          "description": "SSH host for certificate access"
        },
        "redisVector.sshUser": {
          "type": "string",
          "default": "kumarita",
          "description": "SSH username for certificate access"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.60.0",
    "@types/node": "16.x",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "eslint": "^8.20.0",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "redis": "^4.6.7",
    "@tensorflow/tfjs-node": "^4.10.0",
    "@tensorflow-models/universal-sentence-encoder": "^1.3.3"
  }
}
