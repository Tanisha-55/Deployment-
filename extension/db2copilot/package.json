{
  "name": "copilot-db2-extension",
  "displayName": "Copilot DB2 Extension",
  "description": "DB2 database queries powered by GitHub Copilot",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:copilot-db2.runQuery"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "copilot-db2.runQuery",
        "title": "Run DB2 Query from Markdown",
        "category": "Copilot DB2"
      }
    ],
    "configuration": {
      "title": "Copilot DB2",
      "properties": {
        "copilotDb2.connection": {
          "type": "object",
          "default": {
            "database": "",
            "hostname": "",
            "port": "",
            "username": "",
            "password": "",
            "schema": "DICT"
          },
          "description": "DB2 connection configuration"
        }
      }
    }
  },
  "dependencies": {
    "ibm_db": "^3.1.0",
    "@vscode/copilot": "^1.0.0"
  },
  "devDependencies": {
    "@types/vscode": "^1.74.0",
    "@types/node": "^16.18.0",
    "typescript": "^4.9.0"
  }
}
